# 개발 환경 설정 (SQLite + ORM 테스트)
ENVIRONMENT=development

# ORM 사용 여부 (점진적 전환용)
USE_ORM=true  # ORM 통합 완료! 프로덕션 준비됨

# 데이터베이스 URL (프로덕션용)
# DATABASE_URL=postgresql://user:password@localhost:5432/saas_db

# 로깅 레벨
LOG_LEVEL=INFO

# OpenAI API 설정
OPENAI_API_KEY=your_api_key_here

# Qdrant 설정
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
