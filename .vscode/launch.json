{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: ingest.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/backend/ingest.py",
            "args": [
                "--rebuild"
            ],
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/backend"
            }
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false,
            "env": {
                "PYTHONPATH": "${workspaceFolder}/backend"
            }
        }
    ]
}