{
  "getTicketWithConversations": {
    "schema": {
      "method": "GET",
      "protocol": "https",
      "host": "<%= iparam.freshdesk_domain %>",
      "path": "/api/v2/tickets/<%= context.ticketId %>?include=conversations",
      "headers": {
        "Authorization": "Basic <%= encode(iparam.freshdesk_api_key + ':X') %>",
        "Content-Type": "application/json"
      }
    }
  },
  "getTicketConversations": {
    "schema": {
      "method": "GET",
      "protocol": "https",
      "host": "<%= iparam.freshdesk_domain %>",
      "path": "/api/v2/tickets/<%= context.ticketId %>/conversations?page=<%= context.page %>&per_page=30",
      "headers": {
        "Authorization": "Basic <%= encode(iparam.freshdesk_api_key + ':X') %>",
        "Content-Type": "application/json"
      }
    }
  },
  "getTicketFields": {
    "schema": {
      "method": "GET",
      "protocol": "https",
      "host": "<%= iparam.freshdesk_domain %>",
      "path": "/api/v2/ticket_fields",
      "headers": {
        "Authorization": "Basic <%= encode(iparam.freshdesk_api_key + ':X') %>",
        "Content-Type": "application/json"
      }
    }
  },
  "updateTicket": {
    "schema": {
      "method": "PUT",
      "protocol": "https",
      "host": "<%= iparam.freshdesk_domain %>",
      "path": "/api/v2/tickets/<%= context.ticketId %>",
      "headers": {
        "Authorization": "Basic <%= encode(iparam.freshdesk_api_key + ':X') %>",
        "Content-Type": "application/json"
      }
    }
  },
  "backendApiPost": {
    "schema": {
      "method": "POST",
      "protocol": "https",
      "host": "ameer-timberless-paragogically.ngrok-free.dev",
      "path": "/<%= context.path %>",
      "headers": {
        "Content-Type": "application/json",
        "X-Freshdesk-Domain": "<%= iparam.freshdesk_domain %>",
        "X-Freshdesk-API-Key": "<%= iparam.freshdesk_api_key %>",
        "X-Tenant-ID": "<%= iparam.freshdesk_domain.split('.')[0] %>",
        "X-Platform": "freshdesk"
      }
    }
  },
  "backendApiPut": {
    "schema": {
      "method": "PUT",
      "protocol": "https",
      "host": "ameer-timberless-paragogically.ngrok-free.dev",
      "path": "/<%= context.path %>",
      "headers": {
        "Content-Type": "application/json",
        "X-Freshdesk-Domain": "<%= iparam.freshdesk_domain %>",
        "X-Freshdesk-API-Key": "<%= iparam.freshdesk_api_key %>",
        "X-Tenant-ID": "<%= iparam.freshdesk_domain.split('.')[0] %>",
        "X-Platform": "freshdesk"
      }
    }
  },
  "backendApi": {
    "schema": {
      "method": "GET",
      "protocol": "https",
      "host": "ameer-timberless-paragogically.ngrok-free.dev",
      "path": "/<%= context.path %>",
      "headers": {
        "Content-Type": "application/json",
        "X-Freshdesk-Domain": "<%= iparam.freshdesk_domain %>",
        "X-Freshdesk-API-Key": "<%= iparam.freshdesk_api_key %>",
        "X-Tenant-ID": "<%= iparam.freshdesk_domain.split('.')[0] %>",
        "X-Platform": "freshdesk"
      }
    }
  }
}
