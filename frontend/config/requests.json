{
  "getTicketWithConversations": {
    "schema": {
      "method": "GET",
      "protocol": "https",
      "host": "<%= iparam.freshdesk_domain %>",
      "path": "/api/v2/tickets/<%= context.ticketId %>?include=conversations",
      "headers": {
        "Authorization": "Basic <%= encode(iparam.freshdesk_api_key + ':X') %>",
        "Content-Type": "application/json"
      }
    }
  },
  "backendApiPost": {
    "schema": {
      "method": "POST",
      "protocol": "https",
      "host": "ameer-timberless-paragogically.ngrok-free.dev",
      "path": "/<%= context.path %>",
      "headers": {
        "Content-Type": "application/json",
        "X-Domain": "<%= iparam.freshdesk_domain %>",
        "X-API-Key": "<%= iparam.freshdesk_api_key %>",
        "X-Tenant-ID": "<%= iparam.freshdesk_domain.split('.')[0] %>",
        "X-Platform": "freshdesk"
      }
    }
  },
  "backendApi": {
    "schema": {
      "method": "GET",
      "protocol": "https",
      "host": "ameer-timberless-paragogically.ngrok-free.dev",
      "path": "/<%= context.path %>",
      "headers": {
        "Content-Type": "application/json",
        "X-Domain": "<%= iparam.freshdesk_domain %>",
        "X-API-Key": "<%= iparam.freshdesk_api_key %>",
        "X-Tenant-ID": "<%= iparam.freshdesk_domain.split('.')[0] %>",
        "X-Platform": "freshdesk"
      }
    }
  }
}
