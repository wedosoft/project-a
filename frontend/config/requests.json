{
  "getAgent": {
    "schema": {
      "method": "GET",
      "protocol": "https",
      "host": "<%= iparam.freshdesk_domain %>",
      "path": "/api/v2/agents/<%= context.agentId %>",
      "headers": {
        "Authorization": "Basic <%= encode(iparam.freshdesk_api_key + ':X') %>",
        "Content-Type": "application/json"
      }
    }
  },
  "getGroup": {
    "schema": {
      "method": "GET",
      "protocol": "https",
      "host": "<%= iparam.freshdesk_domain %>",
      "path": "/api/v2/groups/<%= context.groupId %>",
      "headers": {
        "Authorization": "Basic <%= encode(iparam.freshdesk_api_key + ':X') %>",
        "Content-Type": "application/json"
      }
    }
  },
  "getTicketField": {
    "schema": {
      "method": "GET",
      "protocol": "https",
      "host": "<%= iparam.freshdesk_domain %>",
      "path": "/api/v2/ticket_fields?type=<%= context.fieldType %>",
      "headers": {
        "Authorization": "Basic <%= encode(iparam.freshdesk_api_key + ':X') %>",
        "Content-Type": "application/json"
      }
    }
  },
  "backendApi": {
    "schema": {
      "method": "GET",
      "protocol": "https",
      "host": "ssl.wedosoft.net",
      "path": "/<%= context.path %>",
      "headers": {
        "Content-Type": "application/json",
        "Accept": "application/json",
        "X-Domain": "<%= iparam.freshdesk_domain %>",
        "X-API-Key": "<%= iparam.freshdesk_api_key %>",
        "X-Tenant-ID": "<%= iparam.freshdesk_domain.split('.')[0] %>",
        "X-Platform": "freshdesk"
      }
    }
  },
  "backendApiPost": {
    "schema": {
      "method": "POST",
      "protocol": "https",
      "host": "ssl.wedosoft.net",
      "path": "/<%= context.path %>",
      "headers": {
        "Content-Type": "application/json",
        "Accept": "application/json",
        "X-Domain": "<%= iparam.freshdesk_domain %>",
        "X-API-Key": "<%= iparam.freshdesk_api_key %>",
        "X-Tenant-ID": "<%= iparam.freshdesk_domain.split('.')[0] %>",
        "X-Platform": "freshdesk"
      }
    }
  },
  "backendApiPut": {
    "schema": {
      "method": "PUT",
      "protocol": "https",
      "host": "ssl.wedosoft.net",
      "path": "/<%= context.path %>",
      "headers": {
        "Content-Type": "application/json",
        "Accept": "application/json",
        "X-Domain": "<%= iparam.freshdesk_domain %>",
        "X-API-Key": "<%= iparam.freshdesk_api_key %>",
        "X-Tenant-ID": "<%= iparam.freshdesk_domain.split('.')[0] %>",
        "X-Platform": "freshdesk"
      }
    }
  },
  "backendApiDelete": {
    "schema": {
      "method": "DELETE",
      "protocol": "https",
      "host": "ssl.wedosoft.net",
      "path": "/<%= context.path %>",
      "headers": {
        "Content-Type": "application/json",
        "Accept": "application/json",
        "X-Domain": "<%= iparam.freshdesk_domain %>",
        "X-API-Key": "<%= iparam.freshdesk_api_key %>",
        "X-Tenant-ID": "<%= iparam.freshdesk_domain.split('.')[0] %>",
        "X-Platform": "freshdesk"
      }
    }
  }
}