{
  "backendApi": {
    "schema": {
      "method": "GET",
      "protocol": "${context.backend_protocol}",
      "host": "${context.backend_host}",
      "path": "/${context.path}",
      "headers": {
        "Content-Type": "application/json",
        "X-Freshdesk-Domain": "${context.freshdesk_domain}",
        "X-Freshdesk-API-Key": "${context.freshdesk_api_key}",
        "ngrok-skip-browser-warning": "true"
      }
    }
  },
  "backendApiPost": {
    "schema": {
      "method": "POST",
      "protocol": "${context.backend_protocol}",
      "host": "${context.backend_host}",
      "path": "/${context.path}",
      "headers": {
        "Content-Type": "application/json",
        "X-Freshdesk-Domain": "${context.freshdesk_domain}",
        "X-Freshdesk-API-Key": "${context.freshdesk_api_key}",
        "ngrok-skip-browser-warning": "true"
      }
    }
  }
}
