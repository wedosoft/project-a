{
  "root": true,
  "ignorePatterns": [
    "dist/**",
    "coverage/**",
    ".fdk/**",
    "node_modules/**",
    "app/scripts/copilot-bundle.js",
    "app/scripts/*-bundle.js",
    "**/*.min.js"
  ],
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-console": "off"
  }
}
