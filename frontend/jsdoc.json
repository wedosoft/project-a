{
  "source": {
    "include": [
      "./app/scripts/"
    ],
    "exclude": [
      "./node_modules/",
      "./tests/",
      "./dist/",
      "./coverage/"
    ],
    "includePattern": "\\.(js)$",
    "exclude": ["node_modules/"]
  },
  "opts": {
    "destination": "./docs/api/",
    "recurse": true
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "markdown": {
    "parser": "gfm"
  }
}
